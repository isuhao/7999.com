	var A=[],D="";
	var H=/<a\s+href=(\"|\')?([^>\1]+)\1>([^§]+)§([^§>]+)<\/a>/;
	while(H.test(B)){
		A.push({
			base64:RegExp.$2,content:RegExp.$3,link:decodeURIComponent(RegExp.$4)
		});
		B=B.replace(H,"")
	};
	for(var F=0,C=A.length;F<C;F++){
		D+=" <a href="+A[F].link+' rel="'+A[F].base64+'" onmouseover="this.href=\''+A[F].link+'\'"  onmousedown="this.href=this.rel;">'+A[F].content+"</a>"
	};
